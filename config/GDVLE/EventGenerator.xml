<?xml version="1.0" encoding="ISO-8859-1"?>

<alg_conf>

<!--
Configuration for the EventGenerator EventRecordVisitorI

Configurable Parameters:
.............................................................................................................................
Name                        Type    Opt  Comment                                               Default
.............................................................................................................................
VldContext                  string  No   Validity context
NModules                    int     No   Number of event generation modules in thread
Module-i                    alg     No   Event generation module i (0...NModules)
ILstGen                     alg     No   Interaction list generator (list of interactions that
                                         can be generated by the event generation thread)
XSecModel                   alg     Yes  Cross section model used at the thread                 GPL: XSecModel@[thread name]
-->

  <!--
  ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  Standard threads
  ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  -->

 <param_set name="VLE"> 
     <param type="string" name="VldContext"> </param>
     <param type="int"    name="NModules">   5                                                    </param>
     <param type="alg"    name="Module-0">   genie::InitialStateAppender/Default                  </param>
     <param type="alg"    name="Module-1">   genie::VertexGenerator/Default                       </param>
     <param type="alg"    name="Module-2">   genie::IBDKinematicsGenerator/Default                </param>
     <param type="alg"    name="Module-3">   genie::IBDPrimaryLeptonGenerator/Default             </param>
     <param type="alg"    name="Module-4">   genie::IBDHadronicSystemGenerator/Default            </param>
     <param type="alg"    name="ILstGen">    genie::IBDInteractionListGenerator/Default           </param>
 </param_set>
   
   
</alg_conf>

